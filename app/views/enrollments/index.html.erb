<p id="notice"><%= notice %></p>

<h1>Enrollments</h1>

<table>
  <thead>
  <tr>
    <th>User</th>
    <th>Course instance</th>
    <th>Start date</th>
    <th>End date</th>
    <th>Is active</th>
    <th colspan="3"></th>
  </tr>
  </thead>

  <tbody>
  <% @enrollments.each do |enrollment| %>
    <tr>
      <td><%= enrollment.user.email %></td>
      <td><%= enrollment.course_instance.code %></td>
      <td><%= enrollment.start_date %></td>
      <td><%= enrollment.end_date %></td>
      <td><%= enrollment.is_active %></td>
      <td><%= link_to 'Show', enrollment %></td>
      <td><%= link_to 'Edit', edit_enrollment_path(enrollment) %></td>
      <td><%= link_to 'Destroy', enrollment, method: :delete, data: {confirm: 'Are you sure?'} %></td>
    </tr>
  <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Enrollment', new_enrollment_path %>
