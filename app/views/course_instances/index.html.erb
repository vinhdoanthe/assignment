<p id="notice"><%= notice %></p>

<h1>Course Instances</h1>

<table>
  <thead>
    <tr>
      <th>Code</th>
      <th>Name</th>
      <th>Description</th>
      <th>Start date</th>
      <th>End date</th>
      <th>Program</th>
      <th>Course</th>
      <th>Status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @course_instances.each do |course_instance| %>
      <tr>
        <td><%= course_instance.code %></td>
        <td><%= course_instance.name %></td>
        <td><%= course_instance.description %></td>
        <td><%= course_instance.start_date %></td>
        <td><%= course_instance.end_date %></td>
        <td><%= course_instance.program %></td>
        <td><%= course_instance.course %></td>
        <td><%= course_instance.status %></td>
        <td><%= link_to 'Show', course_instance %></td>
        <td><%= link_to 'Edit', edit_course_instance_path(course_instance) %></td>
        <td><%= link_to 'Destroy', course_instance, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Course Instance', new_course_instance_path %>
