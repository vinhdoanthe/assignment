<div class="jumbotron" style="margin-top: 10%">
  <h2 class="lead">Welcome to FUNiX Assignment Processing Center</h2>
  <hr class="my-4">
  <p>Please login to start.</p>
  <%- if devise_mapping.omniauthable? %>
    <%- resource_class.omniauth_providers.each do |provider| %>
      <a href=<%= omniauth_authorize_path(resource_name, provider) %>>
        <button class="login-button btn btn-primary btn-lg">Login</button>
      </a>
    <% end %>
  <% end %>
</div>